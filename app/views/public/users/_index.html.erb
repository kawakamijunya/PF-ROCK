<table class='table'>
  <thead>
    <tr>
      <th>ユーザー名</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= link_to 'Show', public_user_path(user.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
